{
	"name": "code-katas",
	"version": "1.0.0",
	"description": "Code Katas, mostly from codewars",
	"main": "index.ts",
	"scripts": {
		"start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
		"create": "npm run build && npm run test",
		"build": "tsc -p .",
		"test": "npx katas-cli",
		"refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
	},
	"keywords": [
		"kata",
		"code",
		"samples",
		"learning"
	],
	"author": "GeorgeWL",
	"license": "ISC",
	"dependencies": {
		"jest": "^24.9.0"
	},
	"devDependencies": {
		"@types/jest": "^24.0.25",
		"@types/node": "^13.1.6",
		"nodemon": "^2.0.2",
		"ts-node": "^8.6.0",
		"typescript": "^3.7.4"
	}
}
